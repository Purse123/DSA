#include<stdio.h>
#define MAXSIZE 10

int main() {
  int stack_int;
  printf("-----------------------------\n");
  printf("Choose the stack operation\n");
  printf("1. PUSH\n2. POP\n3. TRAVERSE\n4. EXIT\n");
  printf("-----------------------------\n");
  printf("Enter the corresponding integer: ");
  scanf("%d", &stack_int);

  STACK_OPERATION var = stack_int;

  switch (var) {
    case 1:
    printf("Push");
    break;
    case 2:
    printf("Pop");
    break;
    case 3:
    printf("Traverse");
    break;
    case 4:
      break;
    break;
      default:
    printf("Input error");
  }
}
